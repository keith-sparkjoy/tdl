(page "index.html")

(defn todo-item [checked? text]
  (div (label (input :type "checkbox" :checked checked?) text)))

(html
  (head
    (link :href "app.css" :rel "stylesheet"))
  (body
    (h1 "TODO List")
    (todo-item true "Buy groceries")
    (todo-item true "Cook dinner")
    (todo-item nil "Wash dishes")))
